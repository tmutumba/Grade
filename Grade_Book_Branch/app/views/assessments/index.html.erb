<p id="notice"><%= notice %></p>

<h1>Assessments</h1>

<table>
  <thead>
    <tr>
      <th>Assttype</th>
      <th>Actual score</th>
      <th>Expected score</th>
      <th>Course</th>
      <th>Section</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assessments.each do |assessment| %>
      <tr>
        <td><%= assessment.AsstType %></td>
        <td><%= assessment.actual_Score %></td>
        <td><%= assessment.expected_Score %></td>
        <td><%= assessment.course %></td>
        <td><%= assessment.section %></td>
        <td><%= assessment.student %></td>
        <td><%= link_to 'Show', assessment %></td>
        <td><%= link_to 'Edit', edit_assessment_path(assessment) %></td>
        <td><%= link_to 'Destroy', assessment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assessment', new_assessment_path %>
